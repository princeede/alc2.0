<%- include ./includes/header %>

    <div id="page-wrapper">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <h1 class="page-header">

                </h1>
            </div>
            <div class="col-lg-8 col-md-8 col-sm-8">
                <div style="border-radius:0" class="panel panel-default">
                    <div class="panel-heading">
                        <h4>
                            <%= data[0].fullName%>'s Profile</h4>
                    </div>
                    <div class="panel-body">
                        <table width="100%" class="table table-striped table-bordered table-hover">

                            <tr>
                                <td style="width: 30%"> <strong>Admission Number:</strong> </td>
                                <td>
                                    <%= data[0].admissionNumber%>
                                </td>
                            </tr>
                            <tr>
                                <td> <strong>First Name:</strong> </td>
                                <td>
                                    <%= data[0].firstName%>
                                </td>
                            </tr>
                            <tr>
                                <td> <strong>Last Name:</strong> </td>
                                <td>
                                    <%= data[0].lastName%>
                                </td>
                            </tr>
                            <tr>
                                <td><strong>Gender</strong></td>
                                <td>
                                    <%= data[0].gender%>
                                </td>
                            </tr>
                            <tr>
                                <td><strong>Faculty</strong></td>
                                <td>
                                    <%= data[0].faculty.name%>
                                </td>
                            </tr>
                            <tr>
                                <td><strong>Department</strong></td>
                                <td>
                                    <%= data[0].department.name%>
                                </td>
                            </tr>

                        </table>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 ">
                <div>List of All Faculties</div>
                <div>List all departments</div>

            </div>
        </div>

    </div>

    <%- include ./includes/footer %>